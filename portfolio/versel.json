{
    "version":2,
    "builds":[
        {
            "src" : "portfolio/wsgi.py",
            "use" : "@versel/python",
            "config" : { "maxLambdaSize":"15mb", "runtime":"3.10" }
        },
        {
            "src" : "build_files.sh",
            "use" : "@versel/static-build",
            "config" : { "distDir": "staticfiles_build" }
        }
    ],
    "routes" : [
        {
            "src" : "/static/(.*)",
            "dest" : "/static/$1"
        },
        {
            "src" : "/(.*)",
            "dest" : "portfolio/wsgi.py"
        }

    ]
}